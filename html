  <script>
    document.addEventListener('DOMContentLoaded', function() {
      const track = document.getElementById('testimonials-track');
      const prevButton = document.getElementById('prev-testimonial');
      const nextButton = document.getElementById('next-testimonial');
      let currentIndex = 0;
      const slides = track.children;
      const slideWidth = 100 / slides.length;
      // Set initial position
      updateSliderPosition();
      // Add event listeners
      prevButton.addEventListener('click', function() {
        if (currentIndex > 0) {
          currentIndex--;
          updateSliderPosition();
        }
      });
      nextButton.addEventListener('click', function() {
        if (currentIndex < slides.length - 1) {
          currentIndex++;
          updateSliderPosition();
        }
      });
      // Update slider position based on current index
      function updateSliderPosition() {
        // Get different offsets based on screen size
        let offset;
        if (window.innerWidth >= 1024) {
          // For large screens, show 3 at a time
          offset = currentIndex * (100 / 3);
          if (currentIndex >= slides.length - 3) {
            offset = (slides.length - 3) * (100 / 3);
          }
        } else if (window.innerWidth >= 768) {
          // For medium screens, show 2 at a time
          offset = currentIndex * (100 / 2);
          if (currentIndex >= slides.length - 2) {
            offset = (slides.length - 2) * (100 / 2);
          }
        } else {
          // For small screens, show 1 at a time
          offset = currentIndex * 100;
        }
        track.style.transform = `translateX(-${offset}%)`;
        // Update button states
        prevButton.disabled = currentIndex === 0;
        prevButton.classList.toggle('opacity-50', currentIndex === 0);
        if (window.innerWidth >= 1024) {
          nextButton.disabled = currentIndex >= slides.length - 3;
          nextButton.classList.toggle('opacity-50', currentIndex >= slides.length - 3);
        } else if (window.innerWidth >= 768) {
          nextButton.disabled = currentIndex >= slides.length - 2;
          nextButton.classList.toggle('opacity-50', currentIndex >= slides.length - 2);
        } else {
          nextButton.disabled = currentIndex >= slides.length - 1;
          nextButton.classList.toggle('opacity-50', currentIndex >= slides.length - 1);
        }
      }
      // Update on window resize
      window.addEventListener('resize', updateSliderPosition);
    });
  </script>
</div>
